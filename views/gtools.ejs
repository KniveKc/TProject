                                <button id="open" class="btn btn-primary">Port öffnen!</button>
                                <button id="close" class="btn btn-primary">Port schließen!</button>
                                <br>
                                <button id="senden" class="btn btn-primary">Befehl senden!</button>
                                <br>
                                <input id="command" type="text" class="form-control" placeholder="Enter command!" />
                                <br>
                                <button id="sendeCode" class="btn btn-primary">Start</button>
                                <button id="pauseCode" class="btn btn-primary">Pause</button>
                                <button id="resumeCode" class="btn btn-primary">Resume</button>
                                <button id="stopCode" class="btn btn-primary">Cancel</button>
                                <br>
                                <label class="btn btn-primary"><span class="glyphicon glyphicon-folder-open"></span> 
                                Browse <input id="ul" type="file" style="position: fixed; top: -100em" accept="text/*"></input>
                                </label>
                                <button id="options" class="btn btn-primary">Optionen!</button>
                                <button id="softReset" class="btn btn-primary">Soft Reset!</button>

                            </div>

                            <div class="row1">
                                <div class="progress-container progress-warning">
                                    <div class="progress">
                                        <div id="progress-bar" class="progress-bar" role="progressbar"></div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>


                <div class="col-md-4">
                    <div class="card">
                        <div class="card-body">
                            <div class="container-fluid">
                                <strong>Konsole</strong>
                                <div id="scrollpre" class="well-sm pre-scrollable">
                                    <ul id="liste" class="v"> </ul>
                                </div>
                            </div>
                        </div>

                        <div id="sliderRegular" class="slider"></div>
                        <div id="sliderDouble" class="slider slider-info"></div>



                    </div>
                </div>



                <div class="col-md-4">
                    <div class="card">
                        <div class="card-body">

                            <button id="xp" class="btn btn-primary">X+</button>
                            <button id="xm" class="btn btn-primary">X-</button>
                            <button id="yp" class="btn btn-primary">Y+</button>
                            <button id="ym" class="btn btn-primary">Y-</button>
                            <button id="unlock" class="btn btn-primary">$X Entsperren!</button>
                            <button id="homing" class="btn btn-primary">$H Homing!</button>

                            <div class="well-lg">
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="row">
                                            <label>|M Position|</label>
                                        </div>
                                        <div class="row">
                                            <label>X:</label>
                                            <strong class="panel panel-default" id="MXPos">0 mm</strong>
                                        </div>

                                        <div class="row">
                                            <label>Y:</label>
                                            <strong class="panel panel-default" id="MYPos">0 mm</strong>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="row">
                                            <label>|W Position|</label>
                                        </div>
                                        <div class="row">
                                            <label>X:</label>
                                            <strong class="panel panel-default" id="WXPos">0 mm</strong>
                                        </div>

                                        <div class="row">
                                            <label>Y:</label>
                                            <strong class="panel panel-default" id="WYPos">0 mm</strong>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="row">
                                            <label>|Work Offset|</label>
                                        </div>
                                        <div class="row">
                                            <label>X:</label>
                                            <strong class="panel panel-default" id="WCOX">0 mm</strong>
                                        </div>

                                        <div class="row">
                                            <label>Y:</label>
                                            <strong class="panel panel-default" id="WCOY">0 mm</strong>
                                        </div>
                                    </div>

                                </div>


                                <div class="row">
                                    <label>|Status:|</label>

                                    <strong id="status" style="color: darkred">Status</strong>
                                </div>


                                <div class="row">

                                    <input id="sliderx" type="range" min="0.1" max="20" step="0.1" value="0.1"></input>
                                    <p id="outx">0.1</p>


                                    <input id="slidery" type="range" min="0.1" max="20" step="0.1" value="0.1"></input>
                                    <p id="outy">0.1</p>

                                </div>

                                <div class="row">
                                    <label>|Feed|</label>
                                </div>
                                <div class="row">
                                    <label>Feed rate: </label>
                                    <strong class="panel panel-default" id="feed">0 mm/min</strong>
                                </div>

                                <div class="row">
                                    <label>Laser Pwm:</label>
                                    <strong class="panel panel-default" id="spindle">0</strong>
                                </div>

                                <div class="row">
                                    <label>|Override|</label>
                                </div>
                                <div class="row">
                                    <label>Ovr Feed:</label>
                                    <strong class="panel panel-default" id="ovf">%</strong>
                                </div>

                                <div class="row">
                                    <label>Ovr Rapid:</label>
                                    <strong class="panel panel-default" id="ovr">%</strong>
                                </div>

                                <div class="row">
                                    <label>Ovr Laser PWM:</label>
                                    <strong class="panel panel-default" id="ovs">%</strong>
                                </div>

                            </div>

                            <button id="goToZero" class="btn btn-primary">Go to Zero</button>
                            <button id="get" class="btn btn-primary">Get</button>

                        </div>
                    </div>
                    <!--Well end -->

                </div>
            </div>




            <div class="row">
                <div class="col-md-4">
                    <div class="card">
                        <div class="card-body">
                            <div class="container-fluid">

                                <div>
                                    <button id="G54" class="btn btn-primary">G54</button>
                                    <button id="G55" class="btn btn-primary">G55</button>
                                    <button id="G56" class="btn btn-primary">G56</button>
                                    <button id="G57" class="btn btn-primary">G57</button>
                                    <button id="G58" class="btn btn-primary">G58</button>
                                    <button id="G59" class="btn btn-primary">G59</button>
                                </div>

                                <div>
                                    <button id="setZero1" class="btn btn-primary">Set G54 Zero</button>
                                    <button id="setZero2" class="btn btn-primary">Set G55 Zero</button>
                                    <button id="setZero3" class="btn btn-primary">Set G56 Zero</button>
                                    <button id="setZero4" class="btn btn-primary">Set G57 Zero</button>
                                    <button id="setZero5" class="btn btn-primary">Set G58 Zero</button>
                                    <button id="setZero6" class="btn btn-primary">Set G59 Zero</button>
                                </div>

                                <div>
                                    <button id="feed_ovr_reset" class="btn btn-primary">F 100%</button>
                                    <button id="feed_ovr_plus" class="btn btn-primary">F +10%</button>
                                    <button id="feed_ovr_minus" class="btn btn-primary">F -10</button>
                                    <button id="feed_ovr_fplus" class="btn btn-primary">F +1%</button>
                                    <button id="feed_ovr_fminus" class="btn btn-primary">F -1%</button>
                                </div>

                                <div>
                                    <button id="rapid_ovr_reset" class="btn btn-primary">R 100%</button>
                                    <button id="rapid_ovr_medium" class="btn btn-primary">R MEDIUM</button>
                                    <button id="rapid_ovr_low" class="btn btn-primary">R LOW</button>

                                    <button id="spindle_ovr_reset" class="btn btn-primary">S 100%</button>
                                    <button id="spindle_ovr_plus" class="btn btn-primary">S +10%</button>
                                    <button id="spindle_ovr_minus" class="btn btn-primary">S -10%</button>
                                    <button id="spindle_ovr_fplus" class="btn btn-primary">S +1%</button>
                                    <button id="spindle_ovr_fminus" class="btn btn-primary">S -1%</button>
                                    <button id="spindle_ovr_stop" class="btn btn-primary">S STOP</button>
                                </div>

                            </div>
                        </div>
                    </div>




                    <div class="col-md-8">
                        <div class="card">
                            <% if (user.username == 'admin') { %>

                                <div class="card">
                                    <div class="card-body">
                                        <strong>Live positioning</strong>
                                        <div>
                                            <strong>X-Axis</strong>
                                            <input id="sliderx1" type="range" min="0.0" max="329" step="0.5" value="0"></input>
                                            <p id="outx1">0.0</p>
                                            <strong>Y-Axis<strong>
                                        
                        <input id="slidery1" type="range" min="0.0" max="382" step="0.5" value="0"></input>
                        <p id="outy1">0.0</p>
                    </div>
                    </div>

                </div>


                <div class="card">
                    <div class="card-body">
                    <div class="btn-group btn-group-justified">
                        <div class="btn-group">
                            <button id="laserOn" class="btn btn-primary">Laser On!</button>
                        </div>
                        <div class="btn-group">
                            <button id="laserOff" class="btn btn-primary">Laser Off!</button>
                        </div>
                    </div>
                        <div>
                        <strong>Laser PWM</strong>
                                            <input id="sliderlaser" type="range" min="9" max="255" step="1" value="0"></input>
                                            <p id="outlaser">0</p>
                                        </div>

                                        <% } else { %>
                                            <% } %>


                                    </div>
                                </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</body>
<footer>

</footer>

